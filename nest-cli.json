{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/skisync/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/skisync/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/skisync",
  "projects": {
    "skisync": {
      "type": "application",
      "root": "apps/skisync",
      "entryFile": "main",
      "sourceRoot": "apps/skisync/src",
      "compilerOptions": {
        "tsConfigPath": "apps/skisync/tsconfig.app.json"
      }
    },
    "smtp": {
      "type": "application",
      "root": "apps/smtp",
      "entryFile": "main",
      "sourceRoot": "apps/smtp/src",
      "compilerOptions": {
        "tsConfigPath": "apps/smtp/tsconfig.app.json"
      }
    },
    "influx-db": {
      "type": "application",
      "root": "apps/influx-db",
      "entryFile": "main",
      "sourceRoot": "apps/influx-db/src",
      "compilerOptions": {
        "tsConfigPath": "apps/influx-db/tsconfig.app.json"
      }
    }
  }
}